<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="con_cuenta_tf">
    <default-activity>cuenta</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="cuenta">
      <page>/Procesos/cuenta.jsff</page>
    </view>
    <view id="cuenta_crud">
      <page>/Procesos/cuenta_crud.jsff</page>
    </view>
    <method-call id="CreateWithParameters">
      <method>#{bindings.CreateWithParams.execute}</method>
      <outcome>
        <fixed-outcome>CreateWithParams</fixed-outcome>
      </outcome>
    </method-call>
    <method-call id="Rollback">
      <method>#{bindings.Rollback.execute}</method>
      <outcome>
        <fixed-outcome>Rollback</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__1">
      <from-activity-id>cuenta</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>crud_cuenta</from-outcome>
        <to-activity-id>CreateWithParameters</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__5">
        <from-outcome>ir_cuenta</from-outcome>
        <to-activity-id>cuenta_crud</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__3">
      <from-activity-id>CreateWithParameters</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome>CreateWithParams</from-outcome>
        <to-activity-id>cuenta_crud</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__6">
      <from-activity-id>cuenta_crud</from-activity-id>
      <control-flow-case id="__7">
        <from-outcome>regresar</from-outcome>
        <to-activity-id>cuenta</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__8">
        <from-outcome>regresar_rollback</from-outcome>
        <to-activity-id>Rollback</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__9">
      <from-activity-id>Rollback</from-activity-id>
      <control-flow-case id="__10">
        <from-outcome>Rollback</from-outcome>
        <to-activity-id>cuenta</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
